#include <iterator>
#include <algorithm>

using namespace std;

inline bool is_good(int num[])
{
	return num[0] * num[0] + num[1] * num[1] == num[2] * num[2] ? true : false;
}

inline bool is_perfect(int num[])
{
	if (!(num[2] - num[1]) || !(num[2] - num[0]) || !(num[1] - num[0]))
		return true;
	return false;
}

int main()
{
	int n, num[3], flag;
	cin >> n;
	while (n--)
	{
		for (int i = 0; i < 3; ++i)
		{
			cin >> num[i];
		}
		sort(&num[0], &num[3]);
// 		copy(&num[0], &num[3], ostream_iterator<int>(cout, " "));
// 		cout << endl;
		if (is_good(num))
			cout << "good\n";
		else if (is_perfect(num))
			cout << "perfect\n";
		else
			cout << "just a triangle\n";
	}
	return 0;
}
